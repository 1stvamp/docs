            li: a(href='https://launchpad.net/juju-core') Juju Core
            li: a(href='https://launchpad.net/charms') Charms
    script(type='text/javascript', src='//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js')
    script(type='text/javascript', src='//ajax.googleapis.com/ajax/libs/jquery/1.8.14/jquery-ui.min.js')
    script(type='text/javascript', src='js/main.js')
